<%= form_with model: prototype, local: true do |f|%>
  <div class="field">
    <%= f.label :title, "タイトル" %><br />
    <%= f.text_field :title %>
  </div>

  <div class="field">
    <%= f.label :place, "場所" %><br />
    <%= f.text_field :place %>
  </div>

  <div class="field">
    <%= f.label :tweet, "ツイート" %><br />
    <%= f.text_area :tweet, class: :form__text %>
  </div>

  <div class="field">
    <div id="previews"></div>
    <%= f.label :image, "画像の添付" %><br />
    <%= f.file_field :image %>
  </div>

  <div class="actions">
    <%= f.submit "保存する", class: :form__btn  %>
  </div>
<% end %>